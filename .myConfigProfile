export MAVEN_HOME=/Users/yuanrongjie/software/coding/apache-maven-3.6.3
export PATH=$PATH:$MAVEN_HOME/bin

# add python path
export PATH=$PATH:/Users/yuanrongjie/Library/Python/3.9/bin

# add mysql path
export PATH=$PATH:/usr/local/mysql/bin

# add go
export GOPATH="$HOME/go"
export PATH="$GOPATH/bin:$PATH"

# from github add openai token
export GITHUB_TOKEN="xx"
